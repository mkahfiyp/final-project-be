{
    "version": 2,
    "builds": [
        {
            "src": "./src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.ts",
            "headers": {
                "Cache-Control": "no-cache, no-store, must-revalidate"
            }
        }
    ]
}